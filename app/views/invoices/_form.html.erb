<div class="col-xs-12">
<%= form_for @invoice do |f| %>
  <div id="invoice-wap">
      <% if @invoice.errors.any? %>
        <div id="error_explanation">
          <h2 class="page-header"><%= pluralize(@invoice.errors.count, "error") %> prohibited this invoice from being saved:</h2>

          <ul>
          <% @invoice.errors.full_messages.each do |message| %>
            <li><%= message %></li>
          <% end %>
          </ul>
        </div>
      <% end %>
    <div class="col-xs-6">
      <div class="form-group">
        <%= f.label :company , required:false %><br>
        <%= f.text_field :company,class:"form-control" %>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group">
        <%= f.label :contragent,required:false  %><br>
        <%= f.text_field :contragent ,class:"form-control"%>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group">      
        <%= f.label :Amount, required:false  %><br>
        <%= f.text_field :amount,class:"form-control" %> <%= f.text_field :currancy, class:"form-control", placeholder:"currancy" %>
      </div>
    </div>
    <div class="col-xs-6">
      <div class="form-group">
        <%= f.label :date, required:false  %><br>
        <div class="col-xs"><%= f.text_field :date, class:"form-control datepicker", as: :string %></div>
      </div>
    </div>
    <div class="form-group">
      <div class="col-xs-10">
        <%= f.submit "submit", class:"btn btn-success" %>
      </div>
    </div>
    <% end %>
  </div>
</div>